<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="535.0"
            prefWidth="963.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="LevelEditorMenuController">
    <children>
        <ImageView fitHeight="550.0" fitWidth="987.0" layoutX="-5.0" layoutY="-3.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@Images/menuBackgroundImage.jpg"/>
            </image>
        </ImageView>
        <Button fx:id="createButton" blendMode="OVERLAY" layoutX="14.0" layoutY="28.0" mnemonicParsing="false"
                onAction="#onButtonClicked" prefHeight="50.0" prefWidth="220.0" text="Create">
            <font>
                <Font name="Felix Titling" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="editButton" blendMode="OVERLAY" layoutX="14.0" layoutY="103.0" mnemonicParsing="false"
                onAction="#onButtonClicked" prefHeight="50.0" prefWidth="220.0" text="Edit">
            <font>
                <Font name="Felix Titling" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="deleteButton" blendMode="OVERLAY" layoutX="14.0" layoutY="471.0" mnemonicParsing="false"
                onAction="#onButtonClicked" prefHeight="50.0" prefWidth="220.0" text="Delete">
            <font>
                <Font name="Felix Titling" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="backButton" blendMode="OVERLAY" layoutX="818.0" layoutY="471.0" mnemonicParsing="false"
                onAction="#onButtonClicked" prefHeight="50.0" prefWidth="125.0" text="Back">
            <font>
                <Font name="Felix Titling" size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
